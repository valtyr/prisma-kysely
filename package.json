{
  "name": "prisma-kysely",
  "description": "Generate Kysely database types from a Prisma schema",
  "repository": {
    "url": "https://github.com/valtyr/prisma-kysely"
  },
  "version": "1.3.0",
  "main": "dist/generator.js",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "bin": {
    "prisma-kysely": "dist/bin.js"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "ttsc --watch",
    "build": "ttsc",
    "typecheck": "ttsc --noemit",
    "prepack": "yarn build",
    "fix": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint ./src",
    "test": "yarn build && vitest --passWithNoTests --coverage",
    "release": "yarn build && yarn changeset publish",
    "edit": "code ."
  },
  "dependencies": {
    "@prisma/generator-helper": "4.13.0",
    "@prisma/internals": "4.13.0",
    "typescript": "4.6.2",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/better-sqlite3": "^7.6.3",
    "@types/node": "17.0.21",
    "@types/pg": "^8.6.6",
    "@types/prettier": "2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitest/coverage-c8": "^0.29.8",
    "better-sqlite3": "^8.2.0",
    "eslint": "^8.35.0",
    "kysely": "^0.23.4",
    "mysql2": "^3.2.0",
    "pg": "^8.10.0",
    "prettier": "^2.8.4",
    "prisma": "4.13.0",
    "ttypescript": "^1.5.15",
    "typescript-transform-paths": "^3.4.6",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.8"
  },
  "prettier": {
    "importOrder": [
      "^\\./env$",
      "<THIRD_PARTY_MODULES>",
      "^~/.*$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
